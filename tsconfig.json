{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react",
    "outDir": "./built",
    "allowJs": true,
    "types": [
      "node",
      "vite/client"
    ],
    "moduleResolution": "node",
    "skipLibCheck": true,
    "target": "es6",
    "lib": [
      "DOM",
      "ES2015"
    ],
    "module": "CommonJS",
    // "strict": true,
  },
  "include": [
    "./client/**/*"
  ]
}